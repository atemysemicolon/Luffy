{
  "manifest_version": 2,
  "name": "Luffy",
  "description": "Get past content restrictions on google",
  "version": "1",
  "author": "The nomadic chef",
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "content_scripts": [{
    "matches": ["*://*.google.com/*"],
    "js": ["jquery.min.js","content.js"]
  }],
  "browser_action": {
    "default_title": "Test Extension"
  },

  "permissions": ["activeTab"]
}